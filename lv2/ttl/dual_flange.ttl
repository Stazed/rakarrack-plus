<https://github.com/Stazed/rakarrack-plus#Dual_Flange>
        a lv2:Plugin, lv2:FlangerPlugin ;
        doap:name "RKRP Dual Flange" ;
        doap:description "Dual Flanger";
        mod:brand "RKRP";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 0 ;
        lv2:microVersion 0 ;
        rdfs:comment "Two flange effects cascaded to create unique and more intense tones than available through a single flanger." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#Dual_Flange_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;

        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "DRYWET" ;
                lv2:name "Dry/Wet" ;
                lv2:default 95 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "PAN" ;
                lv2:name "Pan" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "LRC" ;
                lv2:name "Left/Right Crossover" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "DEPTH" ;
                lv2:name "Depth" ;
                lv2:default 110 ;
                lv2:minimum 20 ;
                lv2:maximum 2500 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "WIDTH" ;
                lv2:name "Sweep Width" ;
                lv2:default 800 ;
                lv2:minimum 0 ;
                lv2:maximum 6000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "OFFS" ;
                lv2:name "Offset Delays";
                lv2:default 10 ;
                lv2:minimum 0 ;
                lv2:maximum 100 ;
                lv2:portProperty lv2:integer ;
                units:unit units:pc ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "FB" ;
                lv2:name "Feedback";
                lv2:default -27 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "LPF" ;
                lv2:name "Lowpass Filter";
                lv2:default 16000 ;
                lv2:minimum 20 ;
                lv2:maximum 20000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "SUB" ;
                lv2:name "Subtract" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "THZ" ;
                lv2:name "Sweep Through Zero" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "TEMPO" ;
                lv2:name "Tempo" ;
                lv2:default 24 ;
                lv2:minimum 1 ;
                lv2:maximum 600 ;
                lv2:portProperty lv2:integer ;
                units:unit units:bpm ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 16 ;
                lv2:symbol "STDL" ;
                lv2:name "LFO L/R Delay" ;
                lv2:default 64 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 17 ;
                lv2:symbol "TYPE" ;
                lv2:name "LFO Type" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 12 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "Sine"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Triangle"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Ramp Up"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "Ramp Down"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "ZigZag"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "Modulated Square"; rdf:value 5 ] ;
                lv2:scalePoint [ rdfs:label "Modulated Saw"; rdf:value 6 ] ;
                lv2:scalePoint [ rdfs:label "Lorenz Fractal"; rdf:value 7 ] ;
                lv2:scalePoint [ rdfs:label "Lorenz Fractal XY"; rdf:value 8 ] ;
                lv2:scalePoint [ rdfs:label "Random Sample and Hold"; rdf:value 9 ] ;
                lv2:scalePoint [ rdfs:label "Triangle Topped Sine"; rdf:value 10 ] ;
                lv2:scalePoint [ rdfs:label "Triangle Bottomed Sine"; rdf:value 11 ] ;
                lv2:scalePoint [ rdfs:label "Static"; rdf:value 12 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 18 ;
                lv2:symbol "RND" ;
                lv2:name "Randomness" ;
                lv2:default 10 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 19 ;
                lv2:symbol "INTENSE" ;
                lv2:name "Intense" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ] ;
	.

