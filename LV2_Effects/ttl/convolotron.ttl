<https://github.com/Stazed/rakarrack-plus#Convolotron:sndfile>
    a lv2:Parameter ;
    rdfs:label "Impulse file" ;
    rdfs:range atom:Path ;
    mediatype: mime:audio ;
    .

<https://github.com/Stazed/rakarrack-plus#Convolotron>
    a lv2:Plugin, lv2:SimulatorPlugin ;
    doap:name "RKRP Convolotron" ;
    doap:description "Convolution-Based Reverb" ;
    lv2:project <https://github.com/Stazed/rakarrack-plus>;
    lv2:minorVersion 3 ;
    lv2:microVersion 1 ;
    rdfs:comment "Convolution-based reverb effect." ;
    lv2:requiredFeature urid:map, worker:schedule ;
    lv2:optionalFeature lv2:hardRTCapable, state:loadDefaultState ;
    lv2:extensionData worker:interface, state:interface ;
#    ui:ui  <https://github.com/Stazed/rakarrack-plus#Convolotron_ui> ;
    patch:writable <https://github.com/Stazed/rakarrack-plus#Convolotron:sndfile> ;

    lv2:port [
            a lv2:InputPort, lv2:AudioPort ;
            lv2:index 0 ;
            lv2:symbol "INPUT_L" ;
            lv2:name "Audio In L" ;
    ] ;
    lv2:port [
            a lv2:InputPort, lv2:AudioPort ;
            lv2:index 1 ;
            lv2:symbol "INPUT_R" ;
            lv2:name "Audio In R" ;
    ] ;
    lv2:port [
            a lv2:OutputPort, lv2:AudioPort ;
            lv2:index 2 ;
            lv2:symbol "OUTPUT_L" ;
            lv2:name "Audio Out L" ;
    ] ;
    lv2:port [
            a lv2:OutputPort, lv2:AudioPort ;
            lv2:index 3 ;
            lv2:symbol "OUTPUT_R" ;
            lv2:name "Audio Out R" ;
    ] ;
    lv2:port [
            a lv2:InputPort, atom:AtomPort ;
            lv2:index 4 ;
            lv2:symbol "CONTROL" ;
            lv2:name "Control" ;
            atom:bufferType atom:Sequence ;
            atom:supports patch:Message ;
            lv2:designation lv2:control ;
    ] ;
    lv2:port [
            a lv2:OutputPort, atom:AtomPort ;
            lv2:index 5 ;
            lv2:symbol "NOTIFY" ;
            lv2:name "Notify" ;
            atom:bufferType atom:Sequence ;
            atom:supports patch:Message ;
            lv2:designation lv2:control ;
    ] ;
    lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 7 ;
            lv2:symbol "DRYWET" ;
            lv2:name "Dry/Wet" ;
            lv2:default 60 ;
            lv2:minimum 0 ;
            lv2:maximum 127 ;
            lv2:portProperty lv2:integer ;
    ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 8 ;
            lv2:symbol "PAN" ;
            lv2:name "Pan" ;
            lv2:default 0 ;
            lv2:minimum -64 ;
            lv2:maximum 63 ;
            lv2:portProperty lv2:integer ;
    ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 9 ;
            lv2:symbol "SAFE" ;
            lv2:name "Safe" ;
            lv2:default 1 ;
            lv2:minimum 0 ;
            lv2:maximum 1 ;
            lv2:portProperty lv2:integer ;
            lv2:portProperty lv2:toggled ; 
    ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 10 ;
            lv2:symbol "LENGTH" ;
            lv2:name "Length" ;
            lv2:default 100 ;
            lv2:minimum 5 ;
            lv2:maximum 250 ;
            lv2:portProperty lv2:integer ;
            units:unit units:ms ;
    ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 11 ;
            lv2:symbol "DAMP" ;
            lv2:name "Dampening" ;
            lv2:default 30 ;
            lv2:minimum 0 ;
            lv2:maximum 127 ;
            lv2:portProperty lv2:integer ;
    ], [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 12 ;
            lv2:symbol "LEVEL" ;
            lv2:name "Level" ;
            lv2:default 20 ;
            lv2:minimum 0 ;
            lv2:maximum 127 ;
            lv2:portProperty lv2:integer ;
    ],  [
            a lv2:InputPort, lv2:ControlPort ;
            lv2:index 13 ;
            lv2:symbol "FB" ;
            lv2:name "Feedback" ;
            lv2:default 0 ;
            lv2:minimum -64 ;
            lv2:maximum 64 ;
            lv2:portProperty lv2:integer ;
    ] ;
    state:state [
		<https://github.com/Stazed/rakarrack-plus#Convolotron:sndfile> <Marshall_JCM200.wav>
	] 
	.

<https://github.com/Stazed/rakarrack-plus#Convolotron:preset:Marshall_JCM200>
        a pset:Preset ;
        lv2:appliesTo <https://github.com/Stazed/rakarrack-plus#Convolotron> ;
        rdfs:seeAlso <convolotron_presets.ttl> .

<https://github.com/Stazed/rakarrack-plus#Convolotron:preset:Fender_Superchamp>
        a pset:Preset ;
