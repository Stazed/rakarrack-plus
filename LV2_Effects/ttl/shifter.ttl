<https://github.com/Stazed/rakarrack-plus#Shifter>
        a lv2:Plugin, lv2:PitchPlugin ;
        doap:name "RKRP Shifter" ;
        doap:description "whammy effect with envelope triggering";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 3 ;
        lv2:microVersion 1 ;
        rdfs:comment "Pitch shifter with easy controls for expression pedal controled whammy effects. Also has a mode for envelope based control." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#Shifter_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "DRYWET" ;
                lv2:name "Dry/Wet" ;
                lv2:default 127 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "PAN" ;
                lv2:name "Pan" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                units:unit units:bpm ;
         ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "GAIN" ;
                lv2:name "Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
         ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "ATACK" ;
                lv2:name "Attack" ;
                lv2:default 200 ;
                lv2:minimum 1 ;
                lv2:maximum 2000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "DECAY" ;
                lv2:name "Decay" ;
                lv2:default 200 ;
                lv2:minimum 1 ;
                lv2:maximum 2000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:ms ;
         ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "THRESH" ;
                lv2:name "Threshold" ;
                lv2:default -20 ;
                lv2:minimum -70 ;
                lv2:maximum 20 ;
                lv2:portProperty lv2:integer ;
                units:unit units:db ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "INT" ;
                lv2:name "Interval" ;
                lv2:default 2 ;
                lv2:minimum 0 ;
                lv2:maximum 12 ;
                lv2:portProperty lv2:integer ;
                units:unit units:semitone12TET ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "DN" ;
                lv2:name "Shift Down" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "MODE" ;
                lv2:name "Mode" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 2 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "Trigger"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Whammy"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "Portamento"; rdf:value 2 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "WHAMMY" ;
                lv2:name "Whammy" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ] ;
	.

