<https://github.com/Stazed/rakarrack-plus#eqp:group:a_low>
    a param:ControlGroup ;
    lv2:index 0 ;
    lv2:name "Low Band" ;
    lv2:symbol "low" .
<https://github.com/Stazed/rakarrack-plus#eqp:group:b_mid>
    a param:ControlGroup ;
    lv2:index 1 ;
    lv2:name "Mid Band" ;
    lv2:symbol "mid" .
<https://github.com/Stazed/rakarrack-plus#eqp:group:c_high>
    a param:ControlGroup ;
    lv2:index 2 ;
    lv2:name "High Band" ;
    lv2:symbol "hi" .

<https://github.com/Stazed/rakarrack-plus#eqp>
        a lv2:Plugin, lv2:ParaEQPlugin ;
        doap:name "RKRP Parametric EQ" ;
        doap:description "3 Band Parametric Eq";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 3 ;
        lv2:microVersion 1 ;
        rdfs:comment "3 band parametric peak-filter equalizer." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#eqp_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "GAIN" ;
                lv2:name "Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "LFREQ" ;
                lv2:name "Low Frequency" ;
                lv2:default 200 ;
                lv2:minimum 20 ;
                lv2:maximum 1000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:a_low> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "LGAIN" ;
                lv2:name "Low Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:a_low> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "LQ" ;
                lv2:name "Low Width" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:a_low> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "MFREQ" ;
                lv2:name "Mid Frequency" ;
                lv2:default 800 ;
                lv2:minimum 800 ;
                lv2:maximum 8000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:b_mid> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "MGAIN" ;
                lv2:name "Mid Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:b_mid> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "MQ" ;
                lv2:name "Mid Width" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:b_mid> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "HFREQ" ;
                lv2:name "High Frequency" ;
                lv2:default 12000 ;
                lv2:minimum 6000 ;
                lv2:maximum 26000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:c_high> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "HGAIN" ;
                lv2:name "High Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:c_high> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "HQ" ;
                lv2:name "High Width" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#eqp:group:c_high> ;
        ] ;
	.

<https://github.com/Stazed/rakarrack-plus#eqp:preset:plain>
        a pset:Preset ;
        lv2:appliesTo <https://github.com/Stazed/rakarrack-plus#eqp> ;
        rdfs:label "Plain" ;
        rdfs:seeAlso <eqp_presets.ttl> .

<https://github.com/Stazed/rakarrack-plus#eqp:preset:pop>
