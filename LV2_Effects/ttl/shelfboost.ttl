<https://github.com/Stazed/rakarrack-plus#ShelfBoost>
        a lv2:Plugin, lv2:FilterPlugin ;
        doap:name "RKRP Shelf Boost" ;
        doap:description "Tone Booster";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 3 ;
        lv2:microVersion 1 ;
        rdfs:comment "Low-shelf filter for simple control of your tone." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#ShelfBoost_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "GAIN" ;
                lv2:name "Gain" ;
                lv2:default 127 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "PRES" ;
                lv2:name "Presence" ;
                lv2:default 64 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "TONE" ;
                lv2:name "Tone" ;
                lv2:default 16000 ;
                lv2:minimum 220 ;
                lv2:maximum 16000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "STEREO" ;
                lv2:name "Stereo" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "LEVEL" ;
                lv2:name "Level" ;
                lv2:default 24 ;
                lv2:minimum 1 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ] ;
	.


<https://github.com/Stazed/rakarrack-plus#ShelfBoost:preset:trebble>
        a pset:Preset ;
        lv2:appliesTo <https://github.com/Stazed/rakarrack-plus#ShelfBoost> ;
        rdfs:seeAlso <shelfboost_presets.ttl> .

