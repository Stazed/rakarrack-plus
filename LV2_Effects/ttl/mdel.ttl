<https://github.com/Stazed/rakarrack-plus#delm:group:del1>
    a param:ControlGroup ;
    lv2:index 0 ;
    lv2:name "Delay 1" ;
    lv2:symbol "delline1" .
<https://github.com/Stazed/rakarrack-plus#delm:group:del2>
    a param:ControlGroup ;
    lv2:index 1 ;
    lv2:name "Delay 2" ;
    lv2:symbol "delline2" .

<https://github.com/Stazed/rakarrack-plus#delm>
        a lv2:Plugin, lv2:DelayPlugin ;
        doap:name "RKRP Musical Delay" ;
        mod:brand "RKRP";
        mod:label "Musical Delay";
        doap:description "Tempo Selectable Dual Delay";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 3 ;
        lv2:microVersion 1 ;
        rdfs:comment "Dual delay with delay times selectable by tempo and note subdivision." ;
        lv2:optionalFeature lv2:hardRTCapable,
            <http://lv2plug.in/ns/ext/urid#map>,
            <http://lv2plug.in/ns/ext/buf-size#boundedBlockLength>;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#delm_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "DRYWET" ;
                lv2:name "Dry/Wet" ;
                lv2:default 63 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "PAN1" ;
                lv2:name "Panning 1" ;
                lv2:default -64 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del1> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "DEL1" ;
                lv2:name "Delay 1" ;
                lv2:default 2 ;
                lv2:minimum 1 ;
                lv2:maximum 6 ;
                lv2:portProperty lv2:integer ;
                units:unit units:beat ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del1> ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "1"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "1/2"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "1/3"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "1/4"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "1/5"; rdf:value 5 ] ;
                lv2:scalePoint [ rdfs:label "1/6"; rdf:value 6 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "OFFS" ;
                lv2:name "Delay Offset" ;
                lv2:default 7 ;
                lv2:minimum 1 ;
                lv2:maximum 7 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "1"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "1/2"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "1/3"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "1/4"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "1/5"; rdf:value 5 ] ;
                lv2:scalePoint [ rdfs:label "1/6"; rdf:value 6 ] ;
                lv2:scalePoint [ rdfs:label "0"; rdf:value 7 ] ;
                units:unit units:beat ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "LRCr" ;
                lv2:name "Left/Right Crossover" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "FB1" ;
                lv2:name "Feedback 1" ;
                lv2:default 59 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del1> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "DAMP" ;
                lv2:name "Damping" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "PAN2" ;
                lv2:name "Panning 2" ;
                lv2:default 63 ;
                lv2:minimum -64 ;
                lv2:maximum 63 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del2> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "DEL2" ;
                lv2:name "Delay 2" ;
                lv2:default 4 ;
                lv2:minimum 1 ;
                lv2:maximum 6 ;
                lv2:portProperty lv2:integer ;
                units:unit units:beat ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del2> ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "1"; rdf:value 1 ] ;
                lv2:scalePoint [ rdfs:label "1/2"; rdf:value 2 ] ;
                lv2:scalePoint [ rdfs:label "1/3"; rdf:value 3 ] ;
                lv2:scalePoint [ rdfs:label "1/4"; rdf:value 4 ] ;
                lv2:scalePoint [ rdfs:label "1/5"; rdf:value 5 ] ;
                lv2:scalePoint [ rdfs:label "1/6"; rdf:value 6 ] ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "FB2" ;
                lv2:name "Feedback 2" ;
                lv2:default 59 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del2> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "TEMPO" ;
                lv2:name "Tempo" ;
                lv2:default 106 ;
                lv2:minimum 10 ;
                lv2:maximum 480 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 16 ;
                lv2:symbol "GAIN1" ;
                lv2:name "Gain 1" ;
                lv2:default 75 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del1> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 17 ;
                lv2:symbol "GAIN2" ;
                lv2:name "Gain 2" ;
                lv2:default 75 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#delm:group:del2> ;
        ] ;
	.

