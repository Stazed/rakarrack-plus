<https://github.com/Stazed/rakarrack-plus#Shuffle:group:low>
    a param:ControlGroup ;
    lv2:index 0 ;
    lv2:name "Low Band" ;
    lv2:symbol "low" .
<https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid1>
    a param:ControlGroup ;
    lv2:index 1 ;
    lv2:name "Mid Band 1" ;
    lv2:symbol "mid1" .
<https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid2>
    a param:ControlGroup ;
    lv2:index 2 ;
    lv2:name "Mid Band 2" ;
    lv2:symbol "mid2" .
<https://github.com/Stazed/rakarrack-plus#Shuffle:group:high>
    a param:ControlGroup ;
    lv2:index 3 ;
    lv2:name "High Band" ;
    lv2:symbol "hi" .

<https://github.com/Stazed/rakarrack-plus#Shuffle>
        a lv2:Plugin, lv2:SpatialPlugin ;
        doap:name "RKRP Shuffle" ;
        doap:description "Stereo Shuffler (Mid/Side EQ)";
        lv2:project <https://github.com/Stazed/rakarrack-plus>;
        lv2:minorVersion 3 ;
        lv2:microVersion 1 ;
        rdfs:comment "Creates interesting spatial effects. Converts stereo signal to Mid/Side and applys a parametric four band EQ to M or S channel. Effect based on Stereo Shuffling paper by Michael Gerzon." ;
        lv2:optionalFeature lv2:hardRTCapable ;
#        ui:ui  <https://github.com/Stazed/rakarrack-plus#Shuffle_ui> ;

        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 0 ;
                lv2:symbol "INPUT_L" ;
                lv2:name "Audio In L" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:AudioPort ;
                lv2:index 1 ;
                lv2:symbol "INPUT_R" ;
                lv2:name "Audio In R" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 2 ;
                lv2:symbol "OUTPUT_L" ;
                lv2:name "Audio Out L" ;
        ] ;
        lv2:port [
                a lv2:OutputPort, lv2:AudioPort ;
                lv2:index 3 ;
                lv2:symbol "OUTPUT_R" ;
                lv2:name "Audio Out R" ;
        ] ;
        lv2:port [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 4 ;
                lv2:symbol "BYPASS" ;
                lv2:name "Bypass" ;
                lv2:default 0 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:toggled ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 5 ;
                lv2:symbol "DRYWET" ;
                lv2:name "Dry/Wet" ;
                lv2:default 63 ;
                lv2:minimum 0 ;
                lv2:maximum 127 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 6 ;
                lv2:symbol "LGAIN" ;
                lv2:name "Low Gain" ;
                lv2:default 10 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:low> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 7 ;
                lv2:symbol "M1GAIN" ;
                lv2:name "Mid Low Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid1> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 8 ;
                lv2:symbol "M2GAIN" ;
                lv2:name "Mid High Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid2> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 9 ;
                lv2:symbol "HGAIN" ;
                lv2:name "High Gain" ;
                lv2:default 0 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:high> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 10 ;
                lv2:symbol "LFREQ" ;
                lv2:name "Low Frequency" ;
                lv2:default 600 ;
                lv2:minimum 20 ;
                lv2:maximum 1000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:low> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 11 ;
                lv2:symbol "M1FREQ" ;
                lv2:name "Mid Low Frequency" ;
                lv2:default 1200 ;
                lv2:minimum 400 ;
                lv2:maximum 4000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid1> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 12 ;
                lv2:symbol "M2FREQ" ;
                lv2:name "Mid High Frequency" ;
                lv2:default 2000 ;
                lv2:minimum 1200 ;
                lv2:maximum 8000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:mid2> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 13 ;
                lv2:symbol "HFREQ" ;
                lv2:name "High Frequency" ;
                lv2:default 6000 ;
                lv2:minimum 6000 ;
                lv2:maximum 26000 ;
                lv2:portProperty lv2:integer ;
                units:unit units:hz ;
                pg:group <https://github.com/Stazed/rakarrack-plus#Shuffle:group:high> ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 14 ;
                lv2:symbol "Q" ;
                lv2:name "Width" ;
                lv2:default -14 ;
                lv2:minimum -64 ;
                lv2:maximum 64 ;
                lv2:portProperty lv2:integer ;
        ], [
                a lv2:InputPort, lv2:ControlPort ;
                lv2:index 15 ;
                lv2:symbol "BAND" ;
                lv2:name "Filtered Band" ;
                lv2:default 1 ;
                lv2:minimum 0 ;
                lv2:maximum 1 ;
                lv2:portProperty lv2:integer ;
                lv2:portProperty lv2:enumeration;
                lv2:scalePoint [ rdfs:label "Mid"; rdf:value 0 ] ;
                lv2:scalePoint [ rdfs:label "Side"; rdf:value 1 ] ;
        ] ;
	.


<https://github.com/Stazed/rakarrack-plus#Shuffle:preset:shuffle_1>
        a pset:Preset ;
        lv2:appliesTo <https://github.com/Stazed/rakarrack-plus#Shuffle> ;
        rdfs:seeAlso <shuffle_presets.ttl> .

